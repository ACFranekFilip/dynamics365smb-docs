{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md", "**/**.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
	"searchScope": ["Financials"],
      "breadcrumb_path": "~/toc.yml"
	},
    "fileMetadata": {},
    "template": [],
    "dest": "financialssmb"
  }
}